section.gua-tabs {background: #328AD2; width: 80vw; max-width: 1200px; min-width: 390px; height: 25rem; margin: 4rem auto; box-shadow: 0 0 6px rgba(0,0,0,.4);}
section.gua-tabs article {position: absolute; left: 0; top: 5rem; right: 0; bottom: 0; padding: 1rem 2rem 0; overflow: auto; transition: .7s; transform: scale(0); transform-origin: center right; transition-delay: .1s;}
section.gua-tabs article:before {color: rgba(0,0,0,.2); font-size: 4rem; font-weight: 100; position: absolute; bottom: 1rem; right: 1rem;}
section.gua-tabs h2 {font-size: 2.5rem; font-weight: 600; text-align: center; color: rgba(0,0,0,.6);}
section.gua-tabs  > h2 {height: auto;}
section.gua-tabs h2 img {width: 120px; height: auto; background: #f9f9f9; border: 5px solid rgba(0,0,0,.7); border-radius: 50%; padding: 5px; display: block; margin: 0 auto; box-shadow: 0 0 7px rgba(0,0,0,.5);}
section.gua-tabs h2 span {font-family: Great Vibes; font-weight: 400; display: block; margin-bottom: 1rem;}
 
section.gua-tabs p, dl, ol {line-height: 1.5; font-size: 1.3rem;}
section.gua-tabs ol li {margin: 0 0 .5rem 1rem;}
section.gua-tabs dt {font-size: 1.6rem; font-weight: 600; text-indent: 1.5rem;}
section.gua-tabs nav {background: #fff; width: 100%; height: 5rem; box-shadow: 0 0 6px rgba(0,0,0,.4);}
section.gua-tabs nav:after {content:''; width: 20rem; height: 5rem; background: #BEE3D1; position: absolute; transition: .5s;}
section.gua-tabs input {display: none;}
section.gua-tabs label {width: 25%; float: left; color: #BEE3D1; text-align: center; cursor: pointer; transition: .5s; z-index: 2;}
section.gua-tabs label:hover {color: #1E6743;}
section.gua-tabs label:before {display: block; font-size: 3rem; line-height: 5rem; z-index: 2;}
section.gua-tabs #profile:checked ~ nav [for='profile'], #settings:checked ~ nav [for='settings'], #posts:checked ~ nav [for='posts'], #books:checked ~ nav [for='books'] {color: #1E6743; font-weight: 600;}
section.gua-tabs #settings:checked ~ nav [for='settings'] {}
section.gua-tabs #profile:checked ~ nav [for='profile'] {}
section.gua-tabs #profile:checked ~ nav:after {left: 0;}
section.gua-tabs #settings:checked ~ nav:after {left:25%; border-top: 0 none;}
section.gua-tabs #posts:checked ~ nav:after {left: 50%;}
section.gua-tabs #books:checked ~ nav:after {left: 75%;}
section.gua-tabs #profile:checked ~ .uno, #settings:checked ~ .dos, #posts:checked ~ .tres, #books:checked ~ .cuatro {display: block; transform: scale(1); transition-delay: .5s;}
section.gua-tabs a {color: rgba(0,0,0,.4);}
section.gua-tabs a:hover {color: rgba(0,0,0,.2);}

{% endstyle %}
