                    <tr> 
                      <th>{{ block.settings.heading1 }}</th>
                      <th>{{ block.settings.heading2 }}</th>
                      <th>{{ block.settings.heading3 }}</th>
                      <th>{{ block.settings.heading4 }}</th>
                    </tr>
                {% endfor %}
                {% for block in tab1  %}
                  {% assign product = block.settings.product %}
                  {% assign product2 = block.settings.product2 %}
                   <tr> 
                    <td><p>{{ product.title |split:'-'| last }}</p></td>
                    <td><p>{{ product.title |split:'"'| first }}"</p></td>
                    <td>
                      {% if product != blank %}
                      <a href="{{ product.url }}"><img src="{{ product.featured_image |img_url:'150x' }}">
                        <span>{{ product.selected_or_first_available_variant.sku }}</span></a>
                      {% else %}
                      <span>N/A</span>
                      {% endif %}  
                    </td>
                   
                     <td>
                      {% if product2 != blank %}
                       <a href="{{ product2.url }}"><img src="{{ product2.featured_image |img_url:'100x' }}">
                         <span>{{ product2.selected_or_first_available_variant.sku }}</span></a>
