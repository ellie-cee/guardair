          {
            "type": "text",
            "id": "faq_question_2",
            "label": "Faq question 2"
          },
          {
            "type": "richtext",
            "id": "faq_answer_2",
            "label": "Faq answer 2"
          },
          {
            "type": "text",
            "id": "faq_question_3",
            "label": "Faq question 3"
          },
          {
            "type": "richtext",
            "id": "faq_answer_3",
            "label": "Faq answer 3"
