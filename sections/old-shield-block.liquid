{%- if section.blocks.size > 0 -%}
  <!-- support-container begins -->
  <section class="support-container" data-section-id="{{ section.id }}" data-section-type="shield-block">
    <!-- container begins -->
    <div class="page-width">
      <div class="row">
        {%- for block in section.blocks -%}
          <div class="col-sm-12">
            <!-- support-box begins -->
            <div class="support-box">
              <!-- box-title begins -->
              <div class="box-title"><h3>{{ block.settings.title }}</h3></div>
              <!-- box-title begins -->
              <!-- box-desc begins -->
              <div class="box-desc">{{ block.settings.body }}</div>
              <!-- box-desc ends -->

              <!-- contact_icons begins -->
               {% if block.settings.contact_icon_1 != blank %}
              <div class="contact_container">
                <div class="contact_single">
                  {% if block.settings.contact_icon_1 != blank %}
                    {{ block.settings.contact_icon_1 }}
                  {% endif %}
                  <div>{{  block.settings.contact_text_1 }}</div>
                </div>
                <div class="contact_single">
                  {% if block.settings.contact_icon_2 != blank %}
                    {{ block.settings.contact_icon_2 }}
                  {% endif %}
                  <div>{{  block.settings.contact_text_2 }}</div>
                </div>
                <div class="contact_single">
                  {% if block.settings.contact_icon_3 != blank %}
                  {{ block.settings.contact_icon_3 }}
                  {% endif %}
                  <div><a href="javascript:void(0);" onclick="openLiveChat()">{{  block.settings.contact_text_3 }}</a></div>
                </div>
              </div>
              {% endif %}
              <!-- contact_icons ends -->
              
              <!-- box-links begins -->
              <div class="box-links">
                {{ block.settings.links }}
              </div>
              <!-- box-links ends -->

              <!-- faq begins -->
              {% if block.settings.faq_title != blank %}
              <div class="faq_container">
                {% if block.settings.faq_title != blank %}
                  <div class="box-title">
                    <h3>{{ block.settings.faq_title }}</h3>
                  </div>
                {% endif %}

                {% if block.settings.faq_question_1 != blank %}
                  <div class="faq_single">
                    <input type="checkbox" id="faq_question_1" name="faq_question_1">
                    <label for="faq_question_1" class="faq_question">
                      <span>{{ block.settings.faq_question_1 }}</span>
                      <span class="icon_expand">{% render 'plus-expand' %}</span><span class="icon_expand_minus">{% render 'minus-expand' %}</span>
                    </label>
                    <div class="faq_answer">
                      {{ block.settings.faq_answer_1 }}
                    </div>
                  </div>
                {% endif %}
                {% if block.settings.faq_question_2 != blank %}
                  <div class="faq_single">
                    <input type="checkbox" id="faq_question_2" name="faq_question_2">
                    <label for="faq_question_2" class="faq_question">
                      <span>{{ block.settings.faq_question_2 }}</span>
                      <span class="icon_expand">{% render 'plus-expand' %}</span><span class="icon_expand_minus">{% render 'minus-expand' %}</span>
                    </label>
                    <div class="faq_answer">
                      {{ block.settings.faq_answer_2 }}
                    </div>
                  </div>
                {% endif %}
                {% if block.settings.faq_question_3 != blank %}
                  <div class="faq_single">
                    <input type="checkbox" id="faq_question_3" name="faq_question_3">
                    <label for="faq_question_3" class="faq_question">
                      <span>{{ block.settings.faq_question_3 }}</span>
                      <span class="icon_expand">{% render 'plus-expand' %}</span><span class="icon_expand_minus">{% render 'minus-expand' %}</span>
                    </label>
                    <div class="faq_answer">
                      {{ block.settings.faq_answer_3 }}
                    </div>
                  </div>
                {% endif %}
                {% if block.settings.faq_question_4 != blank %}
                  <div class="faq_single">
                    <input type="checkbox" id="faq_question_4" name="faq_question_4">
                    <label for="faq_question_4" class="faq_question">
                      <span>{{ block.settings.faq_question_4 }}</span>
                      <span class="icon_expand">{% render 'plus-expand' %}</span><span class="icon_expand_minus">{% render 'minus-expand' %}</span>
                    </label>
                    <div class="faq_answer">
                      {{ block.settings.faq_answer_4 }}
                    </div>
                  </div>
                {% endif %}
                {% if block.settings.faq_question_5 != blank %}
                  <div class="faq_single">
                    <input type="checkbox" id="faq_question_5" name="faq_question_5">
                    <label for="faq_question_5" class="faq_question">
                      <span>{{ block.settings.faq_question_5 }}</span>
                      <span class="icon_expand">{% render 'plus-expand' %}</span><span class="icon_expand_minus">{% render 'minus-expand' %}</span>
                    </label>
                    <div class="faq_answer">
                      {{ block.settings.faq_answer_5 }}
                    </div>
                  </div>
                {% endif %}
                <!-- faq bottom text begins -->
                  <div class="faq-bottom-text">{{ block.settings.faq_bottom }}</div>
                <!-- faq bottom text ends -->
              </div>
              {% endif %}
              <!-- faq ends -->
              
            </div>
            <!-- support-box ends -->
          </div>
        {%- endfor -%}
      </div>
    </div>
    <!-- container ends -->
  </section>
  <!-- support-container ends -->
{%- endif -%}
