    "collapsible_content_36xEED": {
      "type": "collapsible-content",
      "blocks": {
        "collapsible_row_zhenCY": {
          "type": "collapsible_row",
          "settings": {
            "heading": "How do I know which pneumatic vacuum I need?",
            "icon": "none",
            "row_content": "<p>Take our pneumatic vacuum <a href=\"https://guardair.com/pages/vacuum-faq#quiz-zMH9Yyo\" title=\"PowerQUAD™ Vacuum Quiz\"><strong>product recommendation quiz</strong></a>. Just answer a few questions to find out which one is best for your specific requirements.</p>",
            "page": ""
          }
        }
      },
