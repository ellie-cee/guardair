      "name": "t:sections.collapsible_content.presets.name",
      "settings": {
        "caption": "Pneumatic Vacuum FAQ",
        "heading": "General",
        "heading_size": "h2",
        "heading_alignment": "left",
        "layout": "none",
        "color_scheme": "",
        "container_color_scheme": "",
        "open_first_collapsible_row": false,
