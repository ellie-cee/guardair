    "rich_text_try8q8": {
      "type": "rich-text",
      "blocks": {
        "text_7BUw73": {
          "type": "text",
          "settings": {
            "text": "<h2>See our <a href=\"https://cdn.shopify.com/s/files/1/2656/7586/files/POWERQUAD_OPERATING_INSTRUCTIONS_v10.pdf?v=1715958123\" target=\"_blank\" title=\"PowerQUAD™ Operating Instructions\"><strong>PowerQUAD™ Operating Instructions Manual</strong></a> for more information.</h2>"
          }
        }
      },
      "block_order": [
